    <rules>
        <fullName>Consultant Negative Response Follow-Up Completed Email</fullName>
        <actions>
            <name>Consultant_Negative_Response_Follow_Up_Completed</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>Customer_Survey__c.NPS__c</field>
            <operation>lessThan</operation>
            <value>7</value>
        </criteriaItems>
        <criteriaItems>
            <field>Customer_Survey__c.Date_of_Follow_Up_Discussion_with_Client__c</field>
            <operation>notEqual</operation>
        </criteriaItems>
        <criteriaItems>
            <field>ATI_Job__c.Division__c</field>
            <operation>equals</operation>
            <value>Consulting</value>
        </criteriaItems>
        <description>Email sent to Customer Service Specialist, Gary Moore, and Ryan Moore after follow-up on negative response surveys</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>Consultant Negative Response Follow-Up Overdue Email</fullName>
        <active>true</active>
        <criteriaItems>
            <field>Customer_Survey__c.NPS__c</field>
            <operation>lessThan</operation>
            <value>7</value>
        </criteriaItems>
        <criteriaItems>
            <field>Customer_Survey__c.Date_of_Follow_Up_Discussion_with_Client__c</field>
            <operation>equals</operation>
        </criteriaItems>
        <criteriaItems>
            <field>ATI_Job__c.Division__c</field>
            <operation>equals</operation>
            <value>Consulting</value>
        </criteriaItems>
        <description>Reminder sent 4 days after receipt of consultant negative response surveys without follow-up</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
        <workflowTimeTriggers>
            <actions>
                <name>Email_alert_for_Consultant_negative_survey_response_follow_up_overdue</name>
                <type>Alert</type>
            </actions>
            <offsetFromField>Customer_Survey__c.CreatedDate</offsetFromField>
            <timeLength>4</timeLength>
            <workflowTimeTriggerUnit>Days</workflowTimeTriggerUnit>
        </workflowTimeTriggers>
    </rules>
