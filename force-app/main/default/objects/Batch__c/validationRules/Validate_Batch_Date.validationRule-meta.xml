<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Validate_Batch_Date</fullName>
    <active>true</active>
    <errorConditionFormula>Batch_Date__c &gt; TODAY()</errorConditionFormula>
    <errorDisplayField>Batch_Date__c</errorDisplayField>
    <errorMessage>Batch Date should not be a future date</errorMessage>
</ValidationRule>
