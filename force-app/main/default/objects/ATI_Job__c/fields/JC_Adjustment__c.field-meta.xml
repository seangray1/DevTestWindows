<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>JC_Adjustment__c</fullName>
    <description>Dollar difference between actual and forecast GP.</description>
    <externalId>false</externalId>
    <formula>Round((( Job_to_Date_Work_Billed_Next_Month_RS__c  * (1- Estimated_Gross_Profit_RS__c ))- Job_to_Date_Costs_Next_Month_Costs_RS__c ),-2)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>JC Adjustment</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Currency</type>
</CustomField>
