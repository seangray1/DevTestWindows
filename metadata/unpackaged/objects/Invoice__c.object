    <validationRules>
        <fullName>Bill_To_Attention_Lookup_Email</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISBLANK(Bill_To_Attention_Lookup__r.Email),
 ISPICKVAL(Delivery_Method__c,&quot;Email&quot;))</errorConditionFormula>
        <errorMessage>Please populate the email address on the Bill To Attention Lookup contact record in order to choose Delivery Method of &apos;Email.&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cannot_Modify_Final_Invoice</fullName>
        <active>false</active>
        <description>Prevents users from editing invoices marked as &quot;Final.&quot;  This validation rule can be bypassed by users with appropriately configured &quot;Invoice Settings&quot; custom settings.</description>
        <errorConditionFormula>AND(
    NOT($Setup.InvoiceSettings__c.Can_Modify_Final_Invoices__c),
    NOT(ISNEW()),
    PRIORVALUE(Is_Final__c) == True )</errorConditionFormula>
        <errorMessage>This invoice has been marked as &apos;Final&apos; and cannot be modified</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cannot_Modify_Final_Invoice1</fullName>
        <active>false</active>
        <description>Prevents users from editing invoices marked as &quot;Final.&quot;  This validation rule can be bypassed by users with appropriately configured &quot;Invoice Settings&quot; custom settings.</description>
        <errorConditionFormula>AND( Is_Final__c = True, PRIORVALUE(Is_Final__c)  =  True )</errorConditionFormula>
        <errorMessage>This invoice has been marked as FINAL and cannot be modified</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cannot_Modify_Synchronized_Invoice</fullName>
        <active>false</active>
        <description>Prevents users from editing invoices which have been sent to Timberline successfully.  This validation rule can be bypassed by users with appropriately configured &quot;Invoice Settings&quot; custom settings.</description>
        <errorConditionFormula>AND(
    NOT($Setup.InvoiceSettings__c.Can_Modify_Final_Invoices__c),
    NOT(ISNEW()),
	OR(
		AND(
			PRIORVALUE(Send_To_Timberline__c) == True,
			NOT(ISCHANGED(Send_To_Timberline__c)),
			NOT(ISCHANGED(Last_Successful_Timberline_Sync__c)) ),
		NOT(ISBLANK(PRIORVALUE(Last_Successful_Timberline_Sync__c))) ))</errorConditionFormula>
        <errorMessage>This invoice is synchronized with Timberline and cannot be further edited.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cannot_Syncrhonize_Dummy_Invoices</fullName>
        <active>true</active>
        <description>Prevents a record from synchronizing with Timberline if it is flagged as a Dummy Invoice</description>
        <errorConditionFormula>Is_Dummy_Invoice__c &amp;&amp; Send_To_Timberline__c</errorConditionFormula>
        <errorMessage>Dummy Invoices cannot be sent to Timberline</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Invoice_Date_cant_be_prior_to_today</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
	Due_Date__c &lt; TODAY(), 

        NOT( ISCHANGED( AE_1_Bonus__c ) ), 
        NOT( ISCHANGED( AE_2_Bonus__c ) ), 

	NOT($Profile.Name = &quot;System Administrator&quot;), 
        NOT($Profile.Name = &quot;Accounting&quot;),
        NOT($Profile.Name = &quot;Collections&quot;)
)</errorConditionFormula>
        <errorMessage>Invoices can only be created/edited if the Invoice Date is today or in the future. Please contact accounting with any questions.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Invoices_only_editable_while_In_Process</fullName>
        <active>true</active>
        <description>Prevents users (except sys-admin/accounting profiles) from editing invoices that are marked Ready for Billing, Final, or Pending. Also prevents Collections from revising invoices unless they have been synced to Timberline.</description>
        <errorConditionFormula>AND( 

NOT(ISCHANGED(AE_1_Bonus__c)),
NOT(ISCHANGED(AE_2_Bonus__c)),

OR(
NOT(ISPICKVAL(PRIORVALUE(Status__c), &apos;In Process&apos;)), NOT(ISPICKVAL(Status__c, &apos;In Process&apos;)) 
),

NOT(AND(ISPICKVAL(PRIORVALUE(Status__c), &apos;In Process&apos;),ISPICKVAL(Status__c, &apos;Ready for Billing&apos;))),

NOT($Profile.Name = &quot;System Administrator&quot;), 
NOT($Profile.Name = &quot;Accounting&quot;)

)</errorConditionFormula>
        <errorMessage>Invoices can only be edited while they are &apos;In Process&apos;. Please contact accounting with any questions.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Only_Accounting_can_make_status_Final</fullName>
        <active>true</active>
        <description>Only accounting and system admins can mark invoices as final or pending</description>
        <errorConditionFormula>(isnew() || ISCHANGED(Status__c)) &amp;&amp; (ISPICKVAL(Status__c, &quot;Final&quot;) || ISPICKVAL(Status__c,&quot;Pending&quot;)) &amp;&amp; ($Profile.Name &lt;&gt; &quot;Accounting&quot; &amp;&amp; $Profile.Name &lt;&gt; &quot;System Administrator&quot;)</errorConditionFormula>
        <errorDisplayField>Status__c</errorDisplayField>
        <errorMessage>Only accounting can change invoice status to Final.</errorMessage>
    </validationRules>
