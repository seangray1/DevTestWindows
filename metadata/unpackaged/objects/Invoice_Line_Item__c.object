    </validationRules>
    <validationRules>
        <fullName>Name_Other_Not_Required</fullName>
        <active>true</active>
        <description>Make sure that other is not entered when not required.</description>
        <errorConditionFormula>AND(NOT(ISPICKVAL(PickListName__c  , &apos;Other&apos;)),  NOT(ISBLANK(Other__c  )))</errorConditionFormula>
        <errorDisplayField>Other__c</errorDisplayField>
        <errorMessage>You must not enter a value, when there is a name selected.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Name_Other_Required</fullName>
        <active>true</active>
        <description>Make sure that other is entered when needed.</description>
        <errorConditionFormula>AND(ISPICKVAL(PickListName__c  , &apos;Other&apos;),  ISBLANK(Other__c  ))</errorConditionFormula>
        <errorDisplayField>Other__c</errorDisplayField>
        <errorMessage>You need to enter a value, when the &quot;Other&quot; option is selected on the name field.</errorMessage>
