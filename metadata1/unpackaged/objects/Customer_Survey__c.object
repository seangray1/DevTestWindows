    <validationRules>
        <fullName>Enter_number_between_0_to_10</fullName>
        <active>true</active>
        <description>Enter number only between 0 to 10</description>
        <errorConditionFormula>OR( NPS__c &lt; 0,NPS__c &gt;10)</errorConditionFormula>
        <errorDisplayField>NPS__c</errorDisplayField>
        <errorMessage>Enter number between 0 to 10</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Require_all_info_to_enter_follow_up_date</fullName>
        <active>true</active>
        <errorConditionFormula>AND( NOT( ISBLANK( Date_of_Follow_Up_Discussion_with_Client__c )), OR( ISBLANK( Summary_of_Discussion_with_Client__c ),  AND( All_deficiencies_addressed__c = FALSE, ISBLANK( Explanation_of_unresolved_deficiencies__c ) ), AND( ISBLANK( Main_Complaints__c ), ISBLANK( Other_Complaints__c ) ) ) )</errorConditionFormula>
        <errorMessage>Check All: You must select or enter at least one complaint, explain any unresolved deficiencies, and enter a summary of the discussion with the client in order to enter a client follow-up discussion date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Require_desc_of_disciplinary_action</fullName>
        <active>true</active>
        <errorConditionFormula>AND( Disciplinary_action_needed__c = TRUE, ISBLANK( Disciplinary_action_description__c ) )</errorConditionFormula>
        <errorDisplayField>Disciplinary_action_description__c</errorDisplayField>
        <errorMessage>You must enter a description for any needed disciplinary action.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Require_plan_for_reoccurring_problem</fullName>
        <active>true</active>
        <errorConditionFormula>AND( Reoccurring_concern_for_this_ATI_person__c  = TRUE, ISBLANK( Plan_of_action_for_reoccurring_concern__c ) )</errorConditionFormula>
        <errorDisplayField>Plan_of_action_for_reoccurring_concern__c</errorDisplayField>
        <errorMessage>You must enter a plan of action for any reoccurring concerns.</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
