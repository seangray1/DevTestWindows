    <validationRules>
        <fullName>LID__cannot_edit_Priority_on_Linkedin_Tasks</fullName>
        <active>false</active>
        <description>Validation Rule not in use</description>
        <errorConditionFormula>false</errorConditionFormula>
        <errorDisplayField>Priority</errorDisplayField>
        <errorMessage>You cannot edit the Priority field on LInkedin Tasks</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LID__cannot_edit_assigned_on_Linkedin_Tasks</fullName>
        <active>false</active>
        <errorConditionFormula>OR(RecordType.Name = &quot;inMail&quot;, RecordType.Name = &quot;Messages&quot;, RecordType.Name = &quot;PointDrive Presentation Created&quot;, RecordType.Name = &quot;PointDrive Presentation Viewed&quot; )
&amp;&amp; (ISCHANGED( OwnerId ))</errorConditionFormula>
        <errorDisplayField>Status</errorDisplayField>
        <errorMessage>You cannot edit the Assigned To field on LinkedIn Tasks</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LID__cannot_edit_status_on_Linkedin_Tasks</fullName>
        <active>false</active>
        <errorConditionFormula>OR(RecordType.Name = &quot;inMail&quot;, RecordType.Name = &quot;Messages&quot;, RecordType.Name = &quot;PointDrive Presentation Created&quot;, RecordType.Name = &quot;PointDrive Presentation Viewed&quot; )
&amp;&amp; (ISCHANGED( Status ))</errorConditionFormula>
        <errorDisplayField>OwnerId</errorDisplayField>
        <errorMessage>You cannot edit the Assigned To field on LinkedIn Tasks</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>MSA_Approval_Process_Validation</fullName>
        <active>true</active>
        <description>Comments need to be filled for MSA/ERA Type</description>
        <errorConditionFormula>AND( ISPICKVAL(Type,&apos;MSA/ERA&apos;),Description == &apos;&apos;)</errorConditionFormula>
        <errorMessage>Comments need to be filled for MSA/ERA Type</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Marketing_Type_Mandatory</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL( Type ,&quot;Marketing Note&quot;) &amp;&amp;  (ISPICKVAL(Marketing_Type__c,&quot;&quot;))  &amp;&amp;  (ISNEW() || DATEVALUE( CreatedDate )  &gt;  DATEVALUE(&quot;2016-11-8&quot;))  &amp;&amp; NOT($User.Username = &quot;csadmin@ameri-tech.com&quot;)</errorConditionFormula>
        <errorDisplayField>Marketing_Type__c</errorDisplayField>
        <errorMessage>You must select Marketing Type when Type equals Marketing Note.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Payment_Call_Type_Required</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
ISPICKVAL( Service_Class__c ,&quot;Payments&quot;), 
ISBLANK(TEXT( Payment_Call_Type__c )) 
)</errorConditionFormula>
        <errorDisplayField>Payment_Call_Type__c</errorDisplayField>
        <errorMessage>You must populate Payment Call Type when Service Class equals Payments.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Service_Class_Required</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
OR( 
ISPICKVAL( Type ,&quot;Contact Center&quot;),ISPICKVAL( Type ,&quot;Customer Service Note&quot;) 
), 
ISBLANK(TEXT(Service_Class__c )))</errorConditionFormula>
        <errorDisplayField>Service_Class__c</errorDisplayField>
        <errorMessage>You must populate Service Class when Type equals Contact Center or Customer Service.</errorMessage>
    </validationRules>
